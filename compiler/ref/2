#!/usr/local/bin/lang

x :- scan Int
print If x < 10 Then
  "x is too small!" Else "x is $x"

low :- -11
upp :- 0h12_345_6093_4_782_091

For x In low..upp Do
  If x = 5 Then
    print "5 spoted"
    Break
  If x = 6 Then
    print "unreachable"
  print x

A :- (x: Int, y: Nat)

pair :- New A -15, 66

print pair

