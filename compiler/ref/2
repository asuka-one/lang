#!/usr/local/bin/lang
x :- scan Int
print if x < 10 then
  "x is too small!" else "x is $x"

low :- -11
upp :- 0h12_345_6093_4_782_091

for x in low..upp do
  if x = 5 then
    print "5 spoted"
    break
  if x = 6 then
    print "unreacheble"
  print $x

A :- (x: Int, y: Nat)

pair :- new A -15, 66

print $pair

