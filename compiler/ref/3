#!/usr/local/bin/lang

FOR USE 'env'; arg IN env args DO
  print $arg
ELSE
  print "thats all!"

apple :- scan Str
IF apple = "apple" THEN
  print "you know what im thinkin about!"
ELSE
  print "thats not an apple"

function x y z : Int -> Int -> Int -> Int :-
  IF y in x..z THEN x + y * z - 100 / 2
  ELSE
    print "those args are not in order: $x, $y, $z"; x + y + z.

# some comment to ignore
print "thats not a comment"

